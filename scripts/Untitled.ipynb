    "def send_file(view, path):\n",
    "    with open(path, 'rb') as f:\n",
    "        data = f.read()\n",
    "    def _write_file(path, data):\n",
    "        with open(path, 'wb') as f:\n",
    "            f.write(data)\n",
    "    return view.apply(_write_file, path, data)"
   ]
